<!DOCTYPE html>
<html lang="en">
<%- include("./partials/head.ejs") %>

<body>
    <h1>Home</h1>
    <%- include("./partials/nav.ejs") %>
    <div>
        <h2>All blogs</h2>
        <% if(blogs.length> 0 ){%>
        <% blogs.forEach(blog=>{ %>
        <h3><%= blog.title %></h3>
        <p><%= blog.body %></p>
        <%}) %> <% }else {%>
        <p>There are no blogs</p>
        <%} %>
    </div>

    <%- include("./partials/footer.ejs") %>
</body>

</html>